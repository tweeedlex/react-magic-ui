{
  "name": "react-magic-ui",
  "version": "0.9.5",
  "description": "A React component library for building magical user interfaces with liquid glass effect",
  "author": "@tweeedlex",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tweeedlex/react-magic-ui.git"
  },
  "bugs": {
    "url": "https://github.com/tweeedlex/react-magic-ui/issues"
  },
  "homepage": "https://github.com/tweeedlex/react-magic-ui#readme",
  "keywords": [
    "react",
    "component-library",
    "design-system",
    "ui-components",
    "tailwindcss",
    "liquid-glass",
    "glassmorphism",
    "storybook"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore --fix",
    "test": "vitest run",
    "test-watch": "vitest",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@storybook/addon-docs": "^10.0.2",
    "@storybook/addon-links": "^10.0.2",
    "@storybook/addon-onboarding": "^10.0.2",
    "@storybook/react-vite": "^10.0.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^10.0.2",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "sass": "^1.86.4",
    "storybook": "^10.0.2",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.7.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}